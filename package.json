{
  "name": "whatsapp-blast",
  "version": "1.0.0",
  "description": "WhatsApp Broadcasting App using Baileys",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "docker:build": "docker build -t whatsapp-blast .",
    "docker:run": "docker run -p 3000:3000 -v ./auth_info_baileys:/app/auth_info_baileys whatsapp-blast",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f whatsapp-blast"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^6.7.8",
    "express": "^4.18.2",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": ["whatsapp", "broadcast", "baileys", "nodejs"],
  "author": "",
  "license": "MIT"
}
