{
  "name": "whatsapp-blast",
  "version": "1.0.0",
  "description": "WhatsApp Broadcasting App using Baileys",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "docker:build": "docker compose build",
    "docker:build:dev": "docker compose -f docker-compose.dev.yml build",
    "docker:start": "docker compose up -d",
    "docker:start:dev": "docker compose -f docker-compose.dev.yml up -d",
    "docker:stop": "docker compose down",
    "docker:stop:dev": "docker compose -f docker-compose.dev.yml down",
    "docker:logs": "docker compose logs -f whatsapp-blast",
    "docker:logs:dev": "docker compose -f docker-compose.dev.yml logs -f whatsapp-blast-dev"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "express": "^4.21.2",
    "multer": "^2.0.0-rc.4",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  },
  "keywords": [
    "whatsapp",
    "broadcast",
    "baileys",
    "nodejs"
  ],
  "author": "",
  "license": "MIT"
}
